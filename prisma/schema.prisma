generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model show {
  id          Int   @id 
  name        String
  overview    String
  genres      String[]
  numseason   Int
  numepisodes Int
  firstaired  DateTime
  lastaired   DateTime
  networks    String[]
  rating      Float
  languages   String[]
  backgroundpath String
}